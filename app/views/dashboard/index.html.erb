   <%= render "shared/header"%>
   <div class="main-panel">
    <%= render "shared/sidebar"%>

    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-3 col-sm-6">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="col-xs-5">
                                    <div class="icon-big icon-success text-center">
                                        <i class="ti-wallet"></i>
                                    </div>
                                </div>
                                <div class="col-xs-7">
                                    <div class="numbers">
                                        <p>Earning</p>
                                        <%= !earn(@car).blank? ? earn(@car) : 0 %>                             
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="col-xs-5">
                                    <div class="icon-big icon-danger text-center">
                                        <i class="ti-pulse"></i>
                                    </div>
                                </div>
                                <div class="col-xs-7">
                                    <div class="numbers">
                                        <p>Expenses</p>
                                        <%= !cost(@car).blank? ? cost(@car) : 0 %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="col-xs-5">
                                    <div class="icon-big icon-success text-center">
                                        <i class="ti-money"></i>
                                    </div>
                                </div>
                                <div class="col-xs-7">
                                    <div class="numbers">
                                        <p>Profit</p>
                                        <%= !profit(@car).blank? ? profit(@car) : 0%>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="card">
                        <div class="content">
                            <div class="row">
                                <div class="col-xs-5">
                                    <div class="icon-big icon-danger text-center">
                                        <i class="ti-loop"></i>
                                    </div>
                                </div>
                                <div class="col-xs-7">
                                    <div class="numbers">
                                        <p>Unpaid Agreement</p>
                                        <%= !unpaid(@car).blank? ? unpaid(@car) : 0 %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="header">
                            <h4 class="title">Vehicle On Rent</h4>
                        </div>
                        <div class="content">
                          <div class="content table-responsive table-full-width">
                            <div class="bootstrap-table">
                                <div class="fixed-table-toolbar">
                                    <div class="pull-left search">
                                        <!-- <input class="form-control" type="text" placeholder="Search"> -->
                                    </div>
                                </div>
                                <div class="fixed-table-container" style="padding-bottom: 0px;">
                                    <div class="fixed-table-body">
                                        <table class="table table-striped table-hover text-center" >
                                            <thead>
                                                <tr>
                                                    <th class="text-center">
                                                        <div class="th-inner ">
                                                            First name/Last name
                                                        </div>
                                                        <div class="fht-cell"></div>
                                                    </th>
                                                    <th class="text-center">
                                                        <div class="th-inner ">
                                                            Cell phone
                                                        </div>
                                                        <div class="fht-cell"></div>
                                                    </th>
                                                    <th class="text-center">
                                                        <div class="th-inner ">
                                                            Start date/End date
                                                        </div>
                                                        <div class="fht-cell">

                                                        </div>
                                                    </th>
                                                    <th class="text-center">
                                                        <div class="th-inner ">
                                                            Amount
                                                        </div>
                                                        <div class="fht-cell"></div>
                                                    </th>
                                                    <th class="text-center">
                                                        <div class="th-inner ">
                                                            Paid / Unpaid
                                                        </div>
                                                        <div class="fht-cell"></div>
                                                    </th>
                                                    <th class="text-center">
                                                        <div class="th-inner ">
                                                            Operation
                                                        </div>
                                                        <div class="fht-cell"></div>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% @agreement.each do |agr|%>
                                                <tr>
                                                    <td style=""><%= agr.fullname %></td>
                                                    <td style=""><%= agr.phone_number %></td>
                                                    <td style=""><%= agr.start_date.to_s.split('UTC')[0] %> / <%= agr.end_date.to_s.split('UTC')[0] %> </td>
                                                    <td style=""><%= agr.price %> </td>
                                                    <% if agr.is_paid == "unpaid" %>
                                                    <td><label class="label label-danger white">Unpaid</label></td>
                                                    <% elsif agr.is_paid == "paid"%>
                                                    <td><label class="label label-success white">Paid</label></td>
                                                    <% else%>
                                                    <td><label class="label label-primary white">Void</label></td>
                                                    <% end %>
                                                    <td>
                                                        <%= render partial: "link",locals: { agr: agr}%>
                                                    </td>
                                                </tr>
                                                <% end %>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="fixed-table-pagination">
                                        <div class="pull-right pagination">
                                            <%= will_paginate @agreement,renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
   <%= render "shared/footer"%>
</div>
