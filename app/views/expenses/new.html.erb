 	<%= render "shared/header"%>
 	<div class="main-panel">
 		<%= render "shared/sidebar"%>

 		<div class="content">
 			<div class="container-fluid">
 				<div class="row">
 					<div class="col-lg-12">
 						<div class="card">
 							<div class="header">
 								<h4 class="title">New Agreement</h4>
 							</div>
 							<div class="content">
 								<%= form_for @expense,url: expenses_url do |f|%>
 								
 								<div class="row">
 									<div class="col-md-6">
 										<div class="form-group">
 											<label>Expense For?</label>
 											<%= f.text_field :name,:class=>"form-control border-input"%>
 										</div>
 									</div>
 									
 								
 									<div class="col-md-6">
 										<div class="form-group">
 											<label>Expense Price</label>
 											<%= f.text_field :price,:class=>"form-control border-input"%>
 										</div>
 									</div>
 								</div>
 								<div class="row">
 									<div class="col-md-12">
 										<div class="form-group">
 											<label>Expense Date</label>
 											<%= f.text_field :datetime ,:id=>"start_time",:class=>"form-control border-input", :readonly => true%>
 											
 										</div>
 									</div>
 									
 								</div>
 								<div class="text-center">
 									<button type="submit" class="btn btn-info btn-fill btn-wd">Continue</button>
 								</div>
 								<div class="clearfix"></div>

 								<% end %>
 							</div>
 						</div>
 					</div>


 				</div>
 			</div>
 		</div>
 		 <%= render "shared/footer"%>
 	</div>
 	<script type="text/javascript">


 		$('#start_time').datetimepicker({
 			dateFormat: "yy-mm-dd",
 			timeFormat:  "hh:mm:ss",
 		});







 	</script>
